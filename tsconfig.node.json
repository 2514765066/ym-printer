{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/**/*",
    "src/renderer/src/components/list-bar/side.vue"
  ],
  "exclude": ["src/renderer"],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "paths": {
      "@/*": ["./src/main/*"],
      "@type": ["./src/type/index"]
    }
  }
}
