{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["src/renderer/**/*", "src/preload/*.d.ts", "src/type/*.ts"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/renderer/src/*"],
      "@manager/*": ["src/renderer/src/browser-window/manager/*"],
      "@print/*": ["src/renderer/src/browser-window/print/*"],
      "@type": ["src/type/index"]
    }
  }
}
